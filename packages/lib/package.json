{
  "name": "@takuma-ru/vue-swipe-modal",
  "version": "4.0.6",
  "private": false,
  "description": "Swipeable Bottom Sheet library for vue2 and vue3",
  "license": "MIT",
  "author": "takuma-ru <kcattdev@gmail.com> (https://github.com/takuma-ru/)",
  "keywords": [
    "vue",
    "vue.js",
    "nuxt",
    "nuxt.js",
    "swipe",
    "swipeble",
    "modal",
    "dialog",
    "bottom sheet",
    "sheet",
    "pwa"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:takuma-ru/vue-swipe-modal.git"
  },
  "bugs": {
    "url": "https://github.com/takuma-ru/vue-swipe-modal/issues",
    "email": "kcattdev@gmail.com"
  },
  "homepage": "https://vue-swipe-modal-docs.takumaru.dev",
  "main": "./dist/swipe-modal.umd.js",
  "module": "./dist/swipe-modal.es.js",
  "exports": {
    "import": "./dist/swipe-modal.es.js",
    "require": "./dist/swipe-modal.umd.js"
  },
  "types": "@types/components/swipe-modal.d.ts",
  "files": [
    "dist/*",
    "@types/*",
    "LICENSE.md"
  ],
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn switch:3 && vue-tsc && vite build",
    "publish:lib": "yarn build && npm publish --access=public",
    "switch:2": "vue-demi-switch 2 vue2",
    "switch:3": "vue-demi-switch 3",
    "preview": "vite preview",
    "version-up:major": "yarn version --major --no-git-tag-version --no-commit-hooks",
    "version-up:minor": "yarn version --minor --no-git-tag-version --no-commit-hooks",
    "version-up:patch": "yarn version --patch --no-git-tag-version --no-commit-hooks"
  },
  "dependencies": {
    "@vueuse/core": "8.9.4",
    "vue-demi": "^0.13.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/composition-api": "^1.7.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.16",
    "vite-plugin-css-injected-by-js": "^1.5.1",
    "vue": "^3.2.25",
    "vue-tsc": "^0.34.7",
    "vue2": "npm:vue@^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
